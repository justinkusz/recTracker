{
  "name": "rectracker",
  "version": "1.0.0",
  "description": "Keep track of media (books, movies, TV shows, etc) recommended to you by others",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"node server/server.js\" \"npm run client\"",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "server-dev": "export NODE_ENV=development && nodemon --ignore 'client/' server/server.js",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha --exit **/tests/*.test.js",
    "test-watch": "nodemon --exec 'npm test'",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "client-dev": "export PORT=3001 && npm run client"
  },
  "engines": {
    "node": "v10.13.0"
  },
  "author": "Justin Kusz",
  "license": "ISC",
  "dependencies": {
    "@braintree/sanitize-url": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.3.15",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "expect": "^23.6.0",
    "mocha": "^5.2.0",
    "supertest": "^3.3.0"
  }
}
